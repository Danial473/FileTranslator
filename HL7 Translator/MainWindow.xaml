<Window x:Class="HL7_Translator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HL7_Translator"
        mc:Ignorable="d"
        Title="HL7 Translator" Height="350" Width="525">
    <Grid>
        <Button Name="TranslateButton" Content="Translate HL7" HorizontalAlignment="Left" Margin="130,183,0,0" VerticalAlignment="Top" Width="250" Height="55" Click="TranslateButton_Click"/>
        <TextBox Name="BatchDateTextBox" HorizontalAlignment="Left" Height="25" Margin="247,77,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="133"/>
        <TextBox Name="BatchNumberTextBox" Text="1" HorizontalAlignment="Left" Height="25" Margin="247,119,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="133"/>
        <Label Content="Batch Date" HorizontalAlignment="Left" Margin="130,76,0,0" VerticalAlignment="Top"/>
        <Label Content="Batch Number" HorizontalAlignment="Left" Margin="130,118,0,0" VerticalAlignment="Top"/>
        <Menu DockPanel.Dock="Top" Margin="0,0,0,296">
            <MenuItem Header="Settings" Click="MenuItem_Click">
            </MenuItem>
        </Menu>
    </Grid>
</Window>
